{
    "version": "1.0.0",
    "name": "lochlite/cms",
    "description": "Lochlite CMS Update, Management and Maintenance Pack.",
    "type": "library",
    "keywords": [
        "lochlite",
        "lochlite-cms",
        "restore",
        "reinstall",
        "update",
        "maintenance",
        "cms",
        "api",
        "plugin",
        "multidomain",
        "management",
        "analysis",
        "framework",
        "pwa"
    ],
    "require": {
        "php": ">=8.0",
        "ext-imap": ">=8.0",
        "ext-json": ">=8.0",
        "ext-mbstring": ">=8.0",
        "ext-openssl": ">=8.0",
        "ext-soap": ">=8.0",
        "elibyy/tcpdf-laravel": "^9.0",
        "guzzlehttp/guzzle": "^7.4.5",
        "inertiajs/inertia-laravel": "^0.6",
        "jenssegers/agent": "^2.6",
        "laravel/fortify": "^1.19",
        "laravel/framework": "^9.21",
        "laravel/jetstream": "^3.0",
        "laravel/sanctum": "^3.0",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.7",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/flysystem-ftp": "^3.0",
        "league/flysystem-sftp-v3": "^3.0",
        "nesbot/carbon": "^2.59",
        "pusher/pusher-php-server": "^7.0",
        "spatie/laravel-analytics": "^4.1",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-database-mail-templates": "^3.5",
        "spatie/laravel-feed": "^4.1",
        "spatie/laravel-newsletter": "^4.11",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-sitemap": "^6.2",
        "spatie/laravel-sluggable": "^3.4",
        "spatie/simple-excel": "^2.1",
        "tightenco/ziggy": "^1.4",
        "torann/geoip": "^3.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "laravel/breeze": "^1.10",
        "fakerphp/faker": "^1.19",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^6.2",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.3"
    },
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Lochlite\\cms\\": "src/",
            "..\\plugins\\": "/"
        }
    },
    "authors": [
        {
            "name": "Igor Macedo Montalv√£o",
            "homepage": "https://www.linkedin.com/in/igmacedo/"
        },
        {
            "name": "The Lochlite & Lochpay Company",
            "homepage": "https://lochlite.com"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Lochlite\\cms\\LochlitecmsProvider"
            ],
			"aliases": {
            "Lochlitecms": "Lochlite\\cms\\Facades\\Lochlitecms"
            }
         }
    }
}
