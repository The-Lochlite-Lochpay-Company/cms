{
    "version": "1.0.0",
    "name": "lochlite/cms",
    "description": "Lochlite CMS Update, Management and Maintenance Pack.",
    "type": "library",
    "keywords": [
        "lochlite",
        "lochlite-cms",
        "restore",
        "reinstall",
        "update",
        "maintenance",
        "cms",
        "api",
        "plugin",
        "multidomain",
        "management",
        "analysis",
        "framework",
        "pwa"
    ],
    "require": {
        "php": "*",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-imap": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-soap": "*",
        "elibyy/tcpdf-laravel": "^11.1",
        "geoip2/geoip2": "^2.13",
        "guzzlehttp/guzzle": "^7.9",
        "inertiajs/inertia-laravel": "^1.3",
        "jenssegers/agent": "^2.6",
        "laravel/fortify": "^1.19",
        "laravel/framework": "^9.19",
        "laravel/jetstream": "^3.0",
        "laravel/sanctum": "^3.0",
        "laravel/slack-notification-channel": "^3.3",
        "laravel/socialite": "^5.16",
        "laravel/tinker": "^2.7",
        "league/flysystem-aws-s3-v3": "^3.28",
        "league/flysystem-ftp": "^3.28",
        "league/flysystem-sftp-v3": "^3.28",
        "nesbot/carbon": "^2.72",
        "pusher/pusher-php-server": "^7.2",
        "spatie/laravel-activitylog": "*",
        "spatie/laravel-analytics": "^4.1",
        "spatie/laravel-backup": "^8.2",
        "spatie/laravel-data": "*",
        "spatie/laravel-database-mail-templates": "^3.6",
        "spatie/laravel-feed": "^4.2",
        "spatie/laravel-newsletter": "^4.11",
        "spatie/laravel-permission": "^6.9",
        "spatie/laravel-sitemap": "^6.4",
        "spatie/laravel-sluggable": "^3.6",
        "spatie/simple-excel": "^2.5",
        "tightenco/ziggy": "^1.8",
        "torann/geoip": "^3.0"
    },
    "require-dev": {
        "laravel/breeze": "^1.10",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Lochlite\\cms\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Igor Macedo Montalv√£o",
            "homepage": "https://www.linkedin.com/in/igmacedo/"
        },
        {
            "name": "The Lochlite & Lochpay Company",
            "homepage": "https://lochlite.com"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Lochlite\\cms\\LochlitecmsProvider"
            ],
			"aliases": {
            "Lochlitecms": "Lochlite\\cms\\Facades\\Lochlitecms"
            }
         }
    }
}
