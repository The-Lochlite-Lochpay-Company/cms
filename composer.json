{
    "name": "lochlite/cms",
    "description": "Lochlite CMS Update, Management and Maintenance Pack.",
    "type": "library",
    "keywords": [
        "lochlite",
        "lochlite-cms",
        "restore",
        "reinstall",
        "update",
        "maintenance",
        "cms",
        "api",
        "plugin",
        "multidomain",
        "management",
        "analysis",
        "framework",
        "pwa"
    ],
    "require": {
        "php": ">=8.2",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-imap": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-soap": "*",
        "elibyy/tcpdf-laravel": "^11.1",
        "geoip2/geoip2": "^3.0",
        "guzzlehttp/guzzle": "^7.9",
        "inertiajs/inertia-laravel": "^1.0",
        "laravel/fortify": "^1.24",
        "laravel/framework": "^11.9",
        "laravel/jetstream": "*",
        "laravel/sanctum": "^4.0",
        "laravel/slack-notification-channel": "^3.3",
        "laravel/socialite": "^5.16",
        "laravel/tinker": "^2.9",
        "league/flysystem-aws-s3-v3": "^3.28",
        "league/flysystem-ftp": "^3.28",
        "league/flysystem-sftp-v3": "^3.28",
        "nesbot/carbon": "^3.8",
        "pusher/pusher-php-server": "^7.2",
        "spatie/laravel-activitylog": "^4.8",
        "spatie/laravel-analytics": "^5.4",
        "spatie/laravel-backup": "^9.0",
        "spatie/laravel-data": "^4.8",
        "spatie/laravel-database-mail-templates": "^3.6",
        "spatie/laravel-feed": "^4.4",
        "spatie/laravel-newsletter": "^5.2",
        "spatie/laravel-permission": "^6.9",
        "spatie/laravel-sitemap": "^7.2",
        "spatie/laravel-sluggable": "^3.6",
        "spatie/simple-excel": "^3.6",
        "tightenco/ziggy": "^2.3",
        "jenssegers/agent": "^2.6",
        "torann/geoip": "^3.0"
    },
    "require-dev": {
        "laravel/breeze": "^1.10",
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Lochlite\\cms\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Igor Macedo Montalv√£o",
            "homepage": "https://www.linkedin.com/in/igmacedo/"
        },
        {
            "name": "The Lochlite & Lochpay Company",
            "homepage": "https://lochlite.com"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Lochlite\\cms\\LochlitecmsProvider"
            ],
			"aliases": {
            "Lochlitecms": "Lochlite\\cms\\Facades\\Lochlitecms"
            }
         }
    }
}
